                    fi

                # ===================== OSADM-010-2 =====================
                elif [[ "$LAB_ID" == "OSADM-010-2" && "$KEY" == "prod-ci-kelompokx-sijax" ]]; then
                    HOST="ubuntu@172.20.10.65"
                    OUT=$(ssh -n -o StrictHostKeyChecking=no "$HOST" \
