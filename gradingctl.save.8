    echo "=============================="

    for key in "${!DATA[@]}"; do
        value="${DATA[$key]}"
        CLIENT_DATA=$(printf '%s\n' "$CLIENT_DATA" | \
            jq --arg k "$key" --arg v "$value" '.client_data[$k] = $v')
